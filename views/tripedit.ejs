<%- include('partials/header', {afterLogin: true}) %>
    <form action="/admin/<%= data.id %>/edit" method="POST">
        <label>Name</label><br>
        <input type="text" name="name" value="<%= data.name %>"><br><br>
        <label>Price</label><br>
        <input type="text" name="price" value="<%= data.price %>"><br><br>
        <label>Location</label><br>
        <input type="text" name="location" value="<%= data.location %>"><br><br>
        <label>Description</label><br>
        <input type="text" name="description" value="<%= data.description %>"><br><br>
        <input type="submit" value="edit">
    </form>